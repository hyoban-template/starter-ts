{
  "name": "pkg-placeholder",
  "type": "module",
  "version": "0.0.0",
  "packageManager": "pnpm@9.0.6",
  "description": "$description$",
  "author": {
    "name": "hyoban",
    "url": "https://github.com/hyoban"
  },
  "license": "MIT",
  "homepage": "https://github.com/hyoban/pkg-placeholder#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hyoban/pkg-placeholder.git"
  },
  "bugs": "https://github.com/hyoban/pkg-placeholder/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "import": {
        "types": "./dist/es/index.d.ts",
        "default": "./dist/es/index.js"
      },
      "require": {
        "types": "./dist/cjs/index.d.cts",
        "default": "./dist/cjs/index.cjs"
      }
    }
  },
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/es/index.js",
  "types": "./dist/es/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "bunchee",
    "dev": "bunchee --watch",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "prepublishOnly": "pnpm run build",
    "test": "vitest",
    "typecheck": "tsc"
  },
  "devDependencies": {
    "@types/node": "20.11.30",
    "bunchee": "^5.1.3",
    "eslint": "^9.1.1",
    "eslint-config-hyoban": "^2.1.1",
    "typescript": "^5.4.5",
    "vitest": "^1.5.2"
  }
}
