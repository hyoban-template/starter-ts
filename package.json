{
	"name": "pkg-placeholder",
	"version": "0.0.0",
	"description": "$description$",
	"keywords": [],
	"homepage": "https://github.com/hyoban/pkg-placeholder#readme",
	"bugs": "https://github.com/hyoban/pkg-placeholder/issues",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/hyoban/pkg-placeholder.git"
	},
	"license": "MIT",
	"author": {
		"name": "hyoban",
		"url": "https://github.com/hyoban"
	},
	"sideEffects": false,
	"type": "module",
	"exports": {
		".": {
			"import": {
				"types": "./dist/es/index.d.ts",
				"default": "./dist/es/index.js"
			},
			"require": {
				"types": "./dist/cjs/index.d.cts",
				"default": "./dist/cjs/index.cjs"
			}
		}
	},
	"main": "./dist/cjs/index.cjs",
	"module": "./dist/es/index.js",
	"types": "./dist/es/index.d.ts",
	"files": [
		"dist"
	],
	"scripts": {
		"build": "bunchee",
		"dev": "bunchee -w",
		"lint": "prettier --check . && eslint .",
		"lint:fix": "prettier --write . && eslint . --fix",
		"prepublishOnly": "pnpm run build",
		"test": "vitest",
		"typecheck": "tsc"
	},
	"devDependencies": {
		"@ianvs/prettier-plugin-sort-imports": "^4.2.1",
		"@types/node": "20.11.30",
		"bunchee": "^5.1.2",
		"eslint": "^9.0.0",
		"eslint-config-hyoban": "^0.7.1",
		"prettier": "^3.2.5",
		"prettier-plugin-curly": "^0.2.1",
		"prettier-plugin-packagejson": "^2.4.14",
		"typescript": "^5.4.5",
		"vitest": "^1.4.0"
	},
	"packageManager": "pnpm@8.15.6"
}
