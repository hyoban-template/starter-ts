{
  "name": "pkg-placeholder",
  "type": "module",
  "version": "0.0.0",
  "packageManager": "pnpm@8.15.4",
  "description": "$description$",
  "author": {
    "name": "hyoban",
    "url": "https://github.com/hyoban"
  },
  "license": "MIT",
  "homepage": "https://github.com/hyoban/pkg-placeholder#readme",
  "repository": "hyoban/pkg-placeholder",
  "bugs": "https://github.com/hyoban/pkg-placeholder/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "bunchee",
    "dev": "bunchee -w",
    "lint": "eslint .",
    "prepare": "simple-git-hooks",
    "prepublishOnly": "pnpm run build",
    "test": "vitest",
    "typecheck": "tsc"
  },
  "devDependencies": {
    "@types/node": "^20.11.27",
    "bunchee": "^4.4.8",
    "eslint": "^8.57.0",
    "eslint-config-hyoban": "^0.2.0",
    "lint-staged": "^15.2.2",
    "release-it": "^17.1.1",
    "release-it-pnpm": "^4.0.2",
    "simple-git-hooks": "^2.10.0",
    "typescript": "^5.4.2",
    "vitest": "^1.3.1"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
